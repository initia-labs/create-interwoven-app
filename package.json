{
  "name": "create-interwoven-app",
  "version": "1.0.1",
  "description": "Create Interwoven applications with InterwovenKit wallet integration",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "create-interwoven-app": "./bin/index.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "format": "prettier --write \"**/*.{js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,json,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run lint && npm run format:check"
  },
  "keywords": [
    "cli",
    "template",
    "scaffolding",
    "InterwovenKit",
    "Initia",
    "blockchain",
    "wallet"
  ],
  "author": "Initia",
  "license": "MIT",
  "files": [
    "bin/",
    "templates/",
    "src/"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.15",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/initia-labs/create-interwoven-app"
  }
}
